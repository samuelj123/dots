# ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

alias gp='cd financeapp/ && nvim'
alias ga='cd ~/dotfiles/bash && nvim aliases'
alias gnc='cd ~/dotfiles/nvim && nvim init2.lua'
alias gbc='cd ~/dotfiles/bash && nvim bashrc'
alias gtc='nvim ~/dotfiles/tmux/tmux.conf'

# Paths
export PATH=${PATH}:~/dotfiles/bin
